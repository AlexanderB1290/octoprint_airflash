<div id="uploadProgressAF" class="progress progress-text-centered">
    <span class="progress-text-back" data-bind="text: uploadProgressText"></span>
    <div class="bar">
        <span class="progress-text-front" data-bind="copyWidth: ':parent :parent :parent :parent form', text: uploadProgressText" style="width: 268px;"></span>
    </div>
</div>
<div class="row-fluid upload-buttons">
    <span class="btn btn-primary fileinput-button span12" data-bind="enable: $root.loginState.isUser() && !isPrinting(), css: {disabled: !$root.loginState.isUser()}" style="margin-bottom: 10px">
        <i class="fa fa-upload"></i>
        <span>Upload to AirFlash</span>
        <input id="gcodeUploadAF" accept="{{ ",".join(supportedExtensions) }}" type="file" name="file" class="fileinput-button" data-bind="enable:  $root.loginState.isUser(), event: {change: $root.afUpload}" aria-disabled="false">
    </span>
</div>

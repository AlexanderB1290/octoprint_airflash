<div id="gcode_upload_progress" class="progress progress-text-centered">
    <span class="progress-text-back" data-bind="text: uploadProgressText"></span>
    <div class="bar">
        <span class="progress-text-front" data-bind="copyWidth: ':parent :parent :parent :parent form', text: uploadProgressText" style="width: 268px;"></span>
    </div>
</div>
<div class="row-fluid">
    <span class="btn btn-primary fileinput-button span12" data-bind="enable: $root.loginState.isUser(), css: {disabled: !$root.loginState.isUser()}" style="margin-bottom: 10px">
        <i class="fa fa-upload"></i>
        <span>Upload to AirFlash</span>
        <input id="gcode_upload" accept=".stl,.gcode,.gco,.g" type="file" name="file" class="fileinput-button" data-bind="enable: loginState.isUser()" aria-disabled="false">
    </span>
</div>
